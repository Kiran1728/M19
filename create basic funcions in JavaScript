    <h2>Welcome to the World of JavaScript</h2>
    <p id="result"></p>

    <script>
        function sumOfTwoNumbers() {
            let x = 5, y = 6;
            return x + y;
        }

        let sum = sumOfTwoNumbers();
        let message = `The sum of x and y is ${sum}`;

        console.log(message);
        document.getElementById("result").textContent = message;
    </script>
